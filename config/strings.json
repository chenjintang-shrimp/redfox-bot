{
    "USER_INFO_TEMPLATE": {
        "template": "User info for {username}:\nid: {id}\nplaymode: {playmode}\npp: {pp}\nglobal rank: #{global_rank}",
        "fields": [
            "username",
            "id",
            "playmode",
            "statistics.pp",
            "statistics.global_rank"
        ]
    },
    "USER_QUERY_ERROR_TEMPLATE": {
        "template": "Error querying user {user}:\nError: {error_msg}",
        "fields": [
            "user",
            "error_msg"
        ]
    },
    "USER_BIND_EXISTING_TEMPLATE":{
        "template": "You have already binded to osu user {username}\nIf you want to change it, please unbind first.",
        "fields": [
            "username"
        ]
    },
    "USER_BIND_SUCCESS_TEMPLATE":{
        "template": "Successfully binded to osu user {username}",
        "fields": [
            "username"
        ]
    }
    
}